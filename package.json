{
  "name": "getter-done",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "Todo app for desktop to getter done, built with electron",
  "private": true,
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "scripts": {
    "build": "webpack",
    "test": "jest --setupFilesAfterEnv ./src/setupTests.js",
    "start:ui:dev": "webpack serve --mode=development",
    "start:electron:dev": "nodemon --dev",
    "start:dev": "yarn build && concurrently --kill-others \"yarn start:ui:dev\" \"yarn start:electron:dev\"",
    "start": "yarn build && ELECTRON_DISABLE_SECURITY_WARNINGS=true electron dist/main.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^@/ui/(.*)": "<rootDir>/src/ui/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "repository": "https://github.com/herrlax/getter-done",
  "author": "Mikael Malmqvist",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@reach/checkbox": "^0.14.0",
    "@reach/dialog": "^0.14.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.1.10",
    "express": "^4.17.1",
    "immer": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.1",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "4.1.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
