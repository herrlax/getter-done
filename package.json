{
  "name": "getter-done",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "Todo app for desktop to getter done, built with electron",
  "private": true,
  "scripts": {
    "build": "webpack",
    "start:ui:dev": "webpack serve --mode=development",
    "start:electron:dev": "nodemon --dev",
    "start:dev": "concurrently --kill-others \"yarn start:ui:dev\" \"yarn start:electron:dev\"",
    "start": "yarn run build && electron dist/main.js"
  },
  "repository": "https://github.com/herrlax/getter-done",
  "author": "Mikael Malmqvist",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "express": "^4.17.1",
    "immer": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.1",
    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.2.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "4.1.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
