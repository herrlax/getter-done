{
  "name": "getter-done",
  "version": "1.0.0",
  "description": "Todo app for desktop to getter done, built with electron",
  "private": true,
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "scripts": {
    "build": "webpack",
    "test": "jest --setupFilesAfterEnv ./src/setupTests.js",
    "start:ui:dev": "webpack serve --mode=development",
    "start:electron:dev": "nodemon --dev",
    "start:dev": "yarn build && concurrently --kill-others \"yarn start:ui:dev\" \"yarn start:electron:dev\"",
    "start": "yarn build && ELECTRON_DISABLE_SECURITY_WARNINGS=true electron dist/main.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^@/ui/(.*)": "<rootDir>/src/ui/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "repository": "https://github.com/herrlax/getter-done",
  "author": "Mikael Malmqvist",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@reach/checkbox": "^0.17.0",
    "@reach/dialog": "^0.17.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "express": "^4.18.1",
    "immer": "^9.0.14",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@emotion/babel-plugin": "^11.9.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "concurrently": "^7.2.0",
    "css-loader": "^6.7.1",
    "electron": "^18.2.4",
    "electron-builder": "^23.0.3",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
